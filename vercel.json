{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://coffeesupliers.shop/api/$1"
    },
    {
      "source": "/oauth2/(.*)",
      "destination": "https://coffeesupliers.shop/oauth2/$1"
    },
    {
      "source": "/static/(.*)",
      "destination": "/static/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
