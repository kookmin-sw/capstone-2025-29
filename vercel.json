{
  "rewrites": [
    { "source": "/oauth2/(.*)", "destination": "https://coffeesupliers.shop/oauth2/$1" },
    { "source": "/login/oauth2/(.*)", "destination": "https://coffeesupliers.shop/login/oauth2/$1" },
    { "source": "/api/(.*)", "destination": "https://coffeesupliers.shop/api/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
